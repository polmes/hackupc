{
	// Required
	"manifest_version": 2,
	"name": "more_than_140",
	"version": "0.0.0",
	"version_name": "0.0.0 pre-alpha",

	// Recommended
	"description": "A plain text description",
	"icons": {
		"16": "icons/16x16.png",
		"128": "icons/128x128.png"
  	},
      
    // Options page setup
    "options_ui": {
        "page" : "options.html",
        
        "chrome_style": true
    },

	// Optional
	"author": ["Ramiro Balado","Alejandro Hernandez", "Pol Mesalles"],
	"background": {
	"scripts":["background.js"],
	// Recommended
	"persistent": false
	},
	"content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["animate.css"],
      "js": ["jquery-1.12.0.min.js","helper.js", "app.js" ]
    }
  ]


}
